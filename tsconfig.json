{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "lib": [
      "dom",
      "es6",
      "scripthost",
      "es2015.proxy"
    ],
    "outDir": "assets/build"
  },
  "noImplicitAny": true,
  "noUnusedLocals": true,
  "noUnusedParameters": true,
  "alwaysStrict": true,
  "sourceMap": true,
  "strictNullChecks": true,
  "exclude": [
    "node_modules"
  ]
}
