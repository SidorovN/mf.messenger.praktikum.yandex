{
  "extends": ["xo",
    "eslint:recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "indent": ["error", 4],
    "max-len": [2, 100],
    "@typescript-eslint/no-unused-vars": "off",
    "no-unused-vars": "off",
    "no-undef": "off",
    "no-useless-call":  "off",
    "no-constructor-return" : "off",
    "camelcase": "off",
    "eqeqeq": "off"
  }
}
