{
  "name": "mf.messenger.praktikum.yandex",
  "version": "1.0.0",
  "description": "Даже законченный проект остаётся только заготовкой, пока им не начнут пользоваться. Но сначала пользователь должен понять, зачем ему пользоваться вашим кодом. В этом помогает файл README.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "build": "ttsc && npm run sassBuild",
    "dev": "nodemon server.js && npm run sassDev && ttsc",
    "sassBuild": "sass src:build",
    "sassDev": "sass --watch src:src",
    "test": "mocha -r ts-node/register 'tests/*.spec.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SidorovN/mf.messenger.praktikum.yandex.git"
  },
  "author": "sidorovN",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SidorovN/mf.messenger.praktikum.yandex/issues"
  },
  "engines": {
    "node": "<=12.x"
  },
  "homepage": "https://github.com/SidorovN/mf.messenger.praktikum.yandex#readme",
  "dependencies": {
    "express": "^4.17.1",
    "sass": "^1.32.7"
  },
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/mocha": "^8.2.1",
    "@types/sinon": "^9.0.10",
    "@zoltu/typescript-transformer-append-js-extension": "^1.0.1",
    "chai": "^4.3.0",
    "husky": "^4.2.5",
    "jsdom": "^16.4.0",
    "mocha": "^8.3.0",
    "nodemon": "^2.0.6",
    "prettier": "^1.19.1",
    "sinon": "^9.2.4",
    "ts-node": "^9.1.1",
    "ttypescript": "^1.5.12",
    "typescript": "^4.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "prettier --write './**/*.js' && git add ."
    }
  }
}
